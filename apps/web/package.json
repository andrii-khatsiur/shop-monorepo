{
  "name": "@apps/web",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env bun run dev:parallel",
    "dev:parallel": "cross-env vinxi dev & bun run open-browser",
    "open-browser": "sleep 1 && open-cli http://localhost:${PORT:-3000}",
    "build": "dotenv -e .env vinxi build",
    "start": "dotenv -e .env vinxi start",
    "version": "vinxi version"
  },
  "dependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.0",
    "@solidjs/start": "^1.1.0",
    "solid-js": "^1.9.5",
    "vinxi": "^0.5.7"
  },
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "open-cli": "^8.0.0"
  }
}
